package CI346;

public class implicitConcurrency {
	
	
	

	
	public static  boolean calculatePrimes(int i) {
		if (i % 2 == 0) 
		{
			if (i == 2) 
			{
				//System.out.println(i);
				return true;
			}
		return false;
		} else {
			for (int j = 3; j < i; j += 2) 
			{
				if (i % j == 0) 
				{
					return false;			
				}
			}
			//System.out.println(i);
			return true;
		}
	}
}
